{
    "name": "woxapp/scaffold",
    "description": "Basic architecture scaffold, can be used to build various REST APIs",
    "type": "project",
    "authors": [
        {
            "name": "WOXAPP",
            "email": "moskalenko@woxapp.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-phalcon": ">=3.2",
        "WoxappGIT/libs_phalcon_restful": "*",
        "ext-yaml": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-json": "*",
        "ext-gearman": "*",
        "ext-redis": "*",
        "ext-memcached": "*",
        "phpunit/phpunit": "^7",
        "firebase/php-jwt": "^5.0",
        "phalcon/devtools": "^3.2",
        "swiftmailer/swiftmailer": "^6.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/validator": "^4.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "WoxappGIT/libs_phalcon_restful",
                "version": "1.0",
                "source": {
                    "url": "https://dobrik@bitbucket.org/WoxappGIT/libs_phalcon_restful.git",
                    "type": "git",
                    "reference": "release-1.0"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Woxapp\\Restful\\": "vendor/WoxappGIT/libs_phalcon_restful/src/"
        }
    }
}
